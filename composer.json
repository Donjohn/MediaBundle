{
    "name" : "donjohn/media-bundle",
    "type": "symfony-bundle",
    "description": "a simple media bundle inspired by sonata media bundle",
    "keywords": ["media", "symfony"],
    "homepage": "https://github.com/Donjohn/MediaBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Donjohn",
            "email": "donjohn.geekzone@gmail.com",
            "role": "Boss"
        }
    ],
	"prefer-stable": true,
    "require" : {
        "php": ">=7.1",
        "ext-gd": "*",
        "ext-fileinfo": "*",
        "doctrine/doctrine-bundle": "^1.6",
        "symfony/form": "^3.4 || ^4.1",
        "symfony/framework-bundle": "^3.4 || ^4.1",
        "symfony/security-bundle": "^3.4 || ^4.1",
        "symfony/twig-bundle": "^3.4 || ^4.1",
        "symfony/filesystem": "^3.4 || ^4.1",
        "symfony/validator": "^3.4 || ^4.1",
        "twig/twig": "^1.28 || ^2.0",
        "symfony/templating": "^3.4 || ^4.1"

    },
    "suggest": {
        "oneup/uploader-bundle": "for the awesome upload of big files feature",
        "liip/imagine-bundle": "to get filters feature and more for your images",
        "api-platform/core": "for the awesome api feature"
    },
    "autoload" : {
        "psr-4" : {
            "Donjohn\\MediaBundle\\" : ""
        }
    }
}
