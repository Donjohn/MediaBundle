<div class="qq-uploader-selector qq-uploader qq-gallery" qq-drop-area-text="{{ 'media.oneup.drop.files'|trans({},'DonjohnMediaBundle') }}">
    <div class="qq-upload-drop-area-selector qq-upload-drop-area" qq-hide-dropzone>
        <span class="qq-upload-drop-area-text-selector"></span>
    </div>
    <div class="row progress qq-total-progress-bar-container-selector">
        <div role="progressbar" aria-valuenow="0" aria-valuemin="0" aria-valuemax="100" class="progress-bar qq-total-progress-bar-selector"></div>
    </div>
    <div class="btn btn-primary qq-upload-button-selector">
        {{ 'media.oneup.upload.file'|trans({},'DonjohnMediaBundle') }}
    </div>
    {#<span class="qq-drop-processing-selector qq-drop-processing">#}
        {#<span>{{ 'media.oneup.processing.files'|trans({},'DonjohnMediaBundle') }}</span>#}
        {#<span class="qq-drop-processing-spinner-selector qq-drop-processing-spinner"></span>#}
    {#</span>#}

    <div class="qq-upload-list-selector row" role="region" aria-live="polite" aria-relevant="additions removals">
        <div class="thumbnail col-lg-2">
            {#<span role="status" class="qq-upload-status-text-selector qq-upload-status-text"></span>#}
            <div class="progress qq-progress-bar-container-selector">
                <div class="progress-bar qq-progress-bar-selector" role="progressbar" aria-valuenow="0" aria-valuemin="0" aria-valuemax="100"></div>
            </div>
            <div class="qq-upload-spinner-selector text-center"><img src="{{ asset('components/fine-uploader/dist/loading.gif') }}"></div>
            <img class="thumbnail qq-thumbnail-selector" qq-max-size="100" qq-server-scale style="top: 0; transform: none;-moz-transform: none; ms-transform: none;-webkit-transform: none;">


            <div class="qq-file-info caption">
                <div class="qq-file-name">
                    <span class="qq-upload-file-selector qq-upload-file"></span>
                    <span class="qq-upload-size-selector qq-upload-size"></span>
                    {#<span class="qq-edit-filename-icon-selector qq-btn qq-edit-filename-icon" aria-label="{{ 'media.oneup.edit_filename'|trans({},'DonjohnMediaBundle') }}"></span>#}
                </div>
                {#<input class="qq-edit-filename-selector qq-edit-filename" tabindex="0" type="text">#}
                <button type="button" class="btn btn-default qq-upload-pause-selector">
                    <span class="glyphicon glyphicon-pause fa fa-pause" aria-label="{{ 'media.oneup.pause'|trans({},'DonjohnMediaBundle') }}"></span>
                </button>
                <button type="button" class="btn btn-default qq-upload-continue-selector qq-upload-retry-selector">
                    <span class="qlyphicon glyphicon-upload fa fa-upload" aria-label="{{ 'media.oneup.continue'|trans({},'DonjohnMediaBundle') }}"></span>
                </button>
                <button type="button" class="btn btn-default qq-upload-cancel-selector">
                    <span class="glyphicon glyphicon-trash fa fa-remove" aria-label="{{ 'media.oneup.cancel'|trans({},'DonjohnMediaBundle') }}"></span>
                </button>
                <button type="button" class="btn btn-default qq-upload-delete-selector">
                    <span class="glyphicon glyphicon-trash fa fa-trash-o" aria-label="{{ 'media.oneup.delete'|trans({},'DonjohnMediaBundle') }}"></span>
                </button>

            </div>
        </div>
    </div>
</div>