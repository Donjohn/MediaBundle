<div class="qq-uploader-selector qq-uploader qq-gallery" qq-drop-area-text="{{ 'media.oneup.drop.files'|trans({},'DonjohnMediaBundle') }}">
    <div class="qq-upload-drop-area-selector qq-upload-drop-area" qq-hide-dropzone>
        <span class="qq-upload-drop-area-text-selector"></span>
    </div>
    <div class="row progress qq-total-progress-bar-container-selector">
        <div role="progressbar" aria-valuenow="0" aria-valuemin="0" aria-valuemax="100" class="progress-bar qq-total-progress-bar-selector"></div>
    </div>
    <div class="btn btn-primary qq-upload-button-selector">
        {{ 'media.oneup.upload.file'|trans({},'DonjohnMediaBundle') }}
    </div>
    <span class="qq-drop-processing-selector qq-drop-processing">
        <span>{{ 'media.oneup.processing.files'|trans({},'DonjohnMediaBundle') }}</span>
        <span class="qq-drop-processing-spinner-selector qq-drop-processing-spinner"></span>
    </span>

    <div class="qq-upload-list-selector row" role="region" aria-live="polite" aria-relevant="additions removals">
        <div class="thumbnail col-lg-2">
            <span role="status" class="qq-upload-status-text-selector qq-upload-status-text"></span>
            <div class="progress qq-progress-bar-container-selector">
                <div class="progress-bar qq-progress-bar-selector" role="progressbar" aria-valuenow="0" aria-valuemin="0" aria-valuemax="100"></div>
            </div>
            <div class="qq-upload-spinner-selector text-center text-center"><img src="{{ asset('components/fine-uploader/dist/loading.gif') }}"></div>
            <img src="" class="thumbnail qq-thumbnail-selector" qq-max-size="100" qq-server-scale style="top: 0; transform: none;-moz-transform: none; ms-transform: none;-webkit-transform: none;">
            <button type="button" class="btn qq-upload-cancel-selector qq-upload-cancel">X</button>

            <div class="qq-file-info caption">
                <div class="qq-file-name">
                    <span class="qq-upload-file-selector qq-upload-file"></span>
                    <span class="qq-edit-filename-icon-selector qq-btn qq-edit-filename-icon" aria-label="{{ 'media.oneup.edit_filename'|trans({},'DonjohnMediaBundle') }}"></span>
                </div>
                <input class="qq-edit-filename-selector qq-edit-filename" tabindex="0" type="text">
                <span class="qq-upload-size-selector qq-upload-size"></span>
                <button type="button" class="btn btn-cancel qq-btn qq-upload-delete-selector qq-upload-delete">
                    <span class="qq-btn qq-delete-icon" aria-label="{{ 'media.oneup.delete'|trans({},'DonjohnMediaBundle') }}"></span>
                </button>
                <button type="button" class="btn btn-action qq-btn qq-upload-pause-selector qq-upload-pause">
                    <span class="qq-btn qq-pause-icon" aria-label="{{ 'media.oneup.pause'|trans({},'DonjohnMediaBundle') }}"></span>
                </button>
                <button type="button" class="btn btn-success qq-btn qq-upload-continue-selector qq-upload-continue qq-upload-retry-selector">
                    <span class="qq-btn qq-continue-icon" aria-label="{{ 'media.oneup.continue'|trans({},'DonjohnMediaBundle') }}"></span>
                </button>
            </div>
        </div>
    </div>

    <dialog class="qq-alert-dialog-selector">
        <div class="qq-dialog-message-selector"></div>
        <div class="qq-dialog-buttons">
            <button type="button" class="btn btn-cancel qq-cancel-button-selector">{{ 'media.oneup.close'|trans({},'DonjohnMediaBundle') }}</button>
        </div>
    </dialog>

    <dialog class="qq-confirm-dialog-selector">
        <div class="qq-dialog-message-selector"></div>
        <div class="qq-dialog-buttons">
            <button type="button" class="btn btn-cancel qq-cancel-button-selector">{{ 'media.oneup.no'|trans({},'DonjohnMediaBundle') }}</button>
            <button type="button" class="btn btn-success qq-ok-button-selector">{{ 'media.oneup.yes'|trans({},'DonjohnMediaBundle') }}</button>
        </div>
    </dialog>

    <dialog class="qq-prompt-dialog-selector">
        <div class="qq-dialog-message-selector"></div>
        <input type="text">
        <div class="qq-dialog-buttons">
            <button type="button" class="btn btn-cancel qq-cancel-button-selector">{{ 'media.oneup.cancel'|trans({},'DonjohnMediaBundle') }}</button>
            <button type="button" class="btn btn-success qq-ok-button-selector">{{ 'media.oneup.ok'|trans({},'DonjohnMediaBundle') }}</button>
        </div>
    </dialog>
</div>